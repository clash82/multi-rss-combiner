{
  "name": "clash82/multi-rss-combiner",
  "description": "Combines many RSS channels into single one (includes also nice page to display all items)",
  "homepage": "https://github.com/clash82/multi-rss-combiner",
  "license": "GPL-2.0-only",
  "type": "project",
  "authors": [
    {
      "name": "Rafa≈Ç Toborek",
      "homepage": "https://kontakt.toborek.info"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-simplexml": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "wolfcast/browser-detection": "^2.9",
    "simplepie/simplepie": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "MultiRssCombiner\\": "src/lib/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.22",
    "phpstan/phpstan": "^1.10.25",
    "rector/rector": "^0.17.6"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
